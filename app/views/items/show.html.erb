<div class="jumbotron">
  <h1 class="display-4">Item: <%= @item.item_name %></h1>

	<p>
	  <strong>Price:</strong>
	  <%= @item.price %>
	</p>

	<p>
	  <strong>Category:</strong>
	  <%= @item.categoryname %>
	</p>

	<div class="btn-group" role="group" aria-label="Basic example">
		<%= link_to 'Edit', edit_item_path(@item), class:"btn btn-primary" %>
		<%= link_to 'Back', items_path, class:"btn btn-secondary" %>
	</div>
</div>

